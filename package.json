    "@testing-library/dom": "^10.4.0",
    "@types/node": "^16.18.126",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "mapbox-gl": "^3.12.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-credit-cards-2": "^1.0.3",
    "react-dom": "^18.2.0",
    "react-map-gl": "^8.0.4",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-simple-maps": "^3.0.0",
